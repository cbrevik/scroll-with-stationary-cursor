{
    "name": "scroll-with-stationary-cursor",
    "displayName": "Scroll With Stationary Cursor",
    "description": "Scroll up an down page while the cursor stays in place relatively.",
    "version": "0.0.1",
    "publisher": "chris",
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.awesomeScrollUp",
        "onCommand:extension.awesomeScrollDown"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.awesomeScrollUp",
                "title": "Awesome Scroll Up"
            },
            {
                "command": "extension.awesomeScrollDown",
                "title": "Awesome Scroll Down"
            }
        ],
        "keybindings": [
            {
                "key": "ctrl+alt+cmd+up",
                "command": "extension.awesomeScrollUp",
                "title": "Awesome Scroll Up"
            },
            {
                "key": "ctrl+alt+cmd+down",
                "command": "extension.awesomeScrollDown",
                "title": "Awesome Scroll Down"
            }
            
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "eslint": "^4.6.1",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
